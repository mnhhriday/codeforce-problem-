import java.util.Scanner;
public class Main{

    public static void main(String[] args){
        Scanner input= new Scanner(System.in);
        int n=input.nextInt();
        int sum=0;
        float avg;
        for(int i=0;i<n;i++){
            int a=input.nextInt();
            sum+=a;
        }
        avg=sum/n;
        System.out.println(avg);




    }



}

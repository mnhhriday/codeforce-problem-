import java.util.Scanner;
public class Main {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);
        long m = input.nextInt();
        long n =input.nextInt();
        long a = input.nextInt();
        long row = (m+a -1)/a;
        long colam = ( n+ a - 1)/ a;
        long sum = row * colam ;
        System.out.println(sum);
        }
    }

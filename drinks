import java.util.Scanner;
public class Main{

    public static void main(String[] args){
        Scanner input= new Scanner(System.in);
        int n=input.nextInt();
       double sum = 0 ;
       double avg;
        for(int i=0;i<n;i++){
            int a=input.nextInt();
             sum+=a;
        }
         avg = sum/n;
        System.out.println(avg);




    }



}
